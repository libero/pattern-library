{%- if nodes is not defined -%}

  {# Do nothing #}

{%- elseif nodes is iterable -%}

  {%- for node in nodes -%}

    {%- if node.template is defined -%}
      {%- include node.template with node.arguments only -%}
    {%- else -%}
      {{- node -}}
    {%- endif -%}

  {%- endfor -%}

{%- else -%}

  {{- nodes -}}

{%- endif -%}
